{
  "name": "boilerplate",
  "version": "1.1.11",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=20.19.0",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "rm -rf logs && cross-env NODE_ENV=development nodemon -r tsconfig-paths/register --exec ts-node ./src/index.ts --files",
    "dev-test-env": "rm -rf logs && cross-env NODE_ENV=test nodemon -r tsconfig-paths/register --exec ts-node ./src/index.ts --files",
    "start": "cross-env NODE_ENV=development node ./dist/src/index.js",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "lint": "eslint ./src/**/*.ts --fix",
    "format": "prettier --write ./**/*.{ts,json}",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false --testPathPattern=src/__tests__ --testPathIgnorePatterns=dist",
    "prisma:format": "prisma format",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "release": "standard-version && git push --follow-tags --no-verify",
    "prepare": "husky install",
    "pre-commit": "yarn lint",
    "pre-push": "yarn release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "cors": "^2",
    "dotenv": "^16",
    "env-var": "^7.5.0",
    "express": "^4",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "mongodb": "^6.13.1",
    "mongodb-memory-server": "^10.1.4",
    "request-ip": "^3.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/cors": "^2",
    "@types/express": "^4",
    "@types/jest": "^29",
    "@types/node": "^20",
    "@types/request-ip": "^0.0.41",
    "@typescript-eslint/eslint-plugin": "^6",
    "cross-env": "^7",
    "eslint": "^8",
    "eslint-config-prettier": "^9",
    "eslint-config-standard-with-typescript": "^43",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-n": "^16",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-promise": "^6",
    "husky": "^9.1.7",
    "jest": "^29",
    "nodemon": "^3",
    "prettier": "^3",
    "prisma": "^6.5.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29",
    "ts-node": "^10",
    "tsc-alias": "^1",
    "tsconfig-paths": "^4",
    "typescript": "~5.3",
    "typescript-transform-paths": "^3"
  }
}
